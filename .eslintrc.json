{
	"extends": [
		"plugin:@wordpress/eslint-plugin/recommended",
		"plugin:@typescript-eslint/recommended",
		"prettier"
	],
	"parser": "@typescript-eslint/parser",
	"env": {
		"browser": true,
		"jquery": true,
		"node": true
	},
	"rules": {
		"@typescript-eslint/no-unused-vars": [
			"error",
			{ "argsIgnorePattern": "^_" }
		],
		"react/react-in-jsx-scope": "off",
		"react/prop-types": "off"
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}
